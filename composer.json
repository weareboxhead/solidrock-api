{
    "name": "boxhead/craft-solidrock-api",
    "description": "Communicate and process data to and from the Solidrock API",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "solidrock"
    ],
    "support": {
        "docs": "https://github.com/weareboxhead/solidrock-api/blob/master/README.md",
        "issues": "https://github.com/weareboxhead/solidrock-api/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boxhead",
            "homepage": "https://boxhead.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "boxhead\\solidrockapi\\": "src/"
        }
    },
    "extra": {
        "name": "Solidrock API",
        "handle": "solidrock-api",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/weareboxhead/solidrock-api/master/CHANGELOG.md",
        "class": "boxhead\\solidrockapi\\SolidrockApi"
    }
}
